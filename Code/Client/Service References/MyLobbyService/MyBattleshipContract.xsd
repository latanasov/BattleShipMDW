      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ReceiveHitResult">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="result" type="xs:boolean" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="EnemyReady">
    <xs:complexType>
      <xs:sequence />
