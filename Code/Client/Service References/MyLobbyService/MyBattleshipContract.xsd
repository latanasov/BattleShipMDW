<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="MyBattleshipContract" elementFormDefault="qualified" targetNamespace="MyBattleshipContract" xmlns:xs="http://www.w3.org/2001/XMLSchema">
